<?xml version="1.0" encoding="UTF-8"?>
<pipeline>
	<driverFactory id="1" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="1"/>
	<driverFactory id="2" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="6"/>
	<driverFactory id="3" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="4"/>
	<driverFactory id="4" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="1"/>
	<driverFactory id="5" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="2">
		<property propName="queueFactory"
	        	className="org.apache.commons.pipeline.util.BlockingQueueFactory$ArrayBlockingQueueFactory"
	            capacity="200000" fair="false"/>
	</driverFactory>
	<driverFactory id="6" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="15">
		<property propName="queueFactory"
	        	className="org.apache.commons.pipeline.util.BlockingQueueFactory$ArrayBlockingQueueFactory"
	            capacity="200000" fair="false"/>
	</driverFactory>
	<driverFactory id="7" className="org.apache.commons.pipeline.driver.ThreadPoolStageDriverFactory" numThreads="1"/>
	
	<stage driverFactoryId="1" className="edu.scripps.fl.pipeline.UngroupStage"/>
